<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/historyRunDetailPropLayoutCell">
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/historyRunDetailPropCellImgBg" android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
            android:background="@drawable/item_icon_cell"/>
    <ImageView
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:id="@+id/historyRunDetailPropCellImgProp" android:layout_alignLeft="@+id/historyRunDetailPropCellImgBg"
            android:layout_alignTop="@+id/historyRunDetailPropCellImgBg" android:layout_marginTop="8dp" android:layout_marginLeft="10dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="X 5"
            android:id="@+id/historyRunDetailPropCellTxtQuantity"
            android:textSize="8dip" android:textColor="#000000" android:layout_alignLeft="@+id/historyRunDetailPropCellImgProp"
            android:layout_below="@+id/historyRunDetailPropCellImgProp" android:layout_marginLeft="8dp" android:layout_marginTop="2dp"
            android:textStyle="bold"/>
</RelativeLayout>